        "react-toastify": "^11.0.3",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@types/file-saver": "^2.0.7",
        "@types/node": "^22.13.4",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.24.1",
        "@typescript-eslint/parser": "^8.24.1",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.20.1",
